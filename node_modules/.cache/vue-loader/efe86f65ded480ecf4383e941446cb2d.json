{"remainingRequest":"D:\\Buildwithangga\\jampang-store\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\Buildwithangga\\jampang-store\\src\\views\\ShoppingCart.vue?vue&type=style&index=0&id=0a4f6c3c&scoped=true&lang=css&","dependencies":[{"path":"D:\\Buildwithangga\\jampang-store\\src\\views\\ShoppingCart.vue","mtime":1592282680785},{"path":"D:\\Buildwithangga\\jampang-store\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\Buildwithangga\\jampang-store\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"D:\\Buildwithangga\\jampang-store\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"D:\\Buildwithangga\\jampang-store\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\Buildwithangga\\jampang-store\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["ShoppingCart.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiNA;AACA;AACA;AACA","file":"ShoppingCart.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n    <div class=\"shopping\">\r\n    <HeaderJampang />\r\n            <!-- Breadcrumb Section Begin -->\r\n    <div class=\"breacrumb-section\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-lg-12 text-left\">\r\n                    <div class=\"breadcrumb-text product-more\">\r\n                    <router-link to=\"/\">\r\n                        <i class=\"fa fa-home\"></i> Home\r\n                        </router-link>\r\n                        <span>Shopping Cart</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <!-- Breadcrumb Section Begin -->\r\n\r\n    <!-- Shopping Cart Section Begin -->\r\n    <section class=\"shopping-cart spad\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-lg-8\">\r\n                    <div class=\"row\">\r\n                        <div class=\"col-lg-12\">\r\n                            <div class=\"cart-table\">\r\n                                <table>\r\n                                    <thead>\r\n                                        <tr>\r\n                                            <th>Image</th>\r\n                                            <th class=\"p-name text-center\">Product Name</th>\r\n                                            <th>Price</th>\r\n                                            <th>Action</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody>\r\n                                        <tr v-for=\"keranjang in keranjangUser\" :key=\"keranjang.id\">\r\n                                            <td class=\"cart-pic first-row\">\r\n                                                <img class=\"img-cart\" :src=\"keranjang.photo\" />\r\n                                            </td>\r\n                                            <td class=\"cart-title first-row text-center\">\r\n                                                <h5>{{ keranjang.name }}</h5>\r\n                                            </td>\r\n                                            <td class=\"p-price first-row\">{{keranjang.price}}</td>\r\n                                            <td @click=\"removeItem(keranjang.id)\" class=\"delete-item\">\r\n                                            <a href=\"#\"><i class=\"material-icons\">\r\n                                              close\r\n                                              </i></a></td>\r\n                                        </tr>\r\n                                   \r\n                                    </tbody>\r\n                                </table>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-lg-8 text-left\">\r\n                            <h4 class=\"mb-4\">\r\n                                Informasi Pembeli:\r\n                            </h4>\r\n                            <div class=\"user-checkout \">\r\n                                <form>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"namaLengkap\">Nama lengkap</label>\r\n                                        <input type=\"text\" class=\"form-control\" id=\"namaLengkap\" aria-describedby=\"namaHelp\" placeholder=\"Masukan Nama\"\r\n                                        v-model=\"customerInfo.name\">\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"namaLengkap\">Email Address</label>\r\n                                        <input type=\"email\" class=\"form-control\" id=\"emailAddress\" aria-describedby=\"emailHelp\" placeholder=\"Masukan Email\"\r\n                                        v-model=\"customerInfo.email\">\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"namaLengkap\">No. HP</label>\r\n                                        <input type=\"text\" class=\"form-control\" id=\"noHP\" aria-describedby=\"noHPHelp\" placeholder=\"Masukan No. HP\"\r\n                                        v-model=\"customerInfo.number\">\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"alamatLengkap\">Alamat Lengkap</label>\r\n                                        <textarea class=\"form-control\" id=\"alamatLengkap\" rows=\"3\"\r\n                                        v-model=\"customerInfo.address\"></textarea>\r\n                                    </div>\r\n                                </form>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"col-lg-4\">\r\n                    <div class=\"row\">\r\n                        <div class=\"col-lg-12\">\r\n                            <div class=\"proceed-checkout\" align=\"left\">\r\n                                <ul>\r\n                                    <li class=\"subtotal\">ID Transaction <span>#LMS123</span></li>\r\n                                    <li class=\"subtotal mt-3\">Subtotal <span>Rp. {{totalHarga}} </span></li>\r\n                                    <li class=\"subtotal mt-3\">Pajak 10%<span>{{tambahPajak}}</span></li>\r\n                                    <li class=\"subtotal mt-3\">Total Biaya <span>Rp. {{totalBiaya}}</span></li>\r\n                                    <li class=\"subtotal mt-3\">Bank Transfer <span>BNI</span></li>\r\n                                    <li class=\"subtotal mt-3\">No. Rekening <span>2209 2000 9991</span></li>\r\n                                    <li class=\"subtotal mt-3\">Nama Penerima <span>Laili Septiandi</span></li>\r\n                                </ul>\r\n                                <!-- <router-link to=\"/success\"> -->\r\n                            <a @click=\"checkout()\" href=\"#\" class=\"proceed-btn\">I ALREADY PAID</a>\r\n                                <!-- </router-link> -->\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n    <!-- Shopping Cart Section End -->\r\n\r\n    \r\n        \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderJampang from '@/components/HeaderJampang.vue';\r\n// import FooterJampang from '@/components/FooterJampang.vue';\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n    name:'cart',\r\n    components: {\r\n        HeaderJampang,\r\n        // FooterJampang\r\n    },\r\n      data() {\r\n    return {\r\n      keranjangUser: [],\r\n      customerInfo: {\r\n          name: '',\r\n          email: '',\r\n          number: '',\r\n          address: ''\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    // removeItem(index){\r\n    //   this.keranjangUser.splice(index, 1);\r\n    //   const parsed = JSON.stringify(this.keranjangUser);\r\n    //   localStorage.setItem(\"keranjangUser\", parsed);\r\n    // },\r\n      removeItem(idx){\r\n\r\n      // mencari item id untuk di hapus\r\n      let keranjangUserStorage = JSON.parse(localStorage.getItem(\"keranjangUser\"));\r\n      let itemKeranjangUserStorage = keranjangUserStorage.map(itemKeranjangUserStorage => itemKeranjangUserStorage.id);\r\n\r\n      // mencocokan idx dengan id storage\r\n      let index = itemKeranjangUserStorage.findIndex(id => id == idx);\r\n      this.keranjangUser.splice(index, 1);\r\n\r\n      const parsed = JSON.stringify(this.keranjangUser);\r\n      localStorage.setItem(\"keranjangUser\", parsed);\r\n      window.location.reload();\r\n    },\r\n  \r\n  checkout(){\r\n    let productIds = this.keranjangUser.map(function(product){\r\n        return product.id;\r\n    });  \r\n    let checkoutData = {\r\n       name: this.customerInfo.name,\r\n        email: this.customerInfo.email,\r\n        number: this.customerInfo.number,\r\n        address: this.customerInfo.address,\r\n        transaction_total: this.totalBiaya,\r\n        transaction_status: \"PENDING\",\r\n        transaction_details: productIds\r\n      };\r\n    axios\r\n      .post(\r\n        \"http://testdev.my.id/api/checkout\", \r\n        checkoutData \r\n        )\r\n      .then(() => this.$router.push(\"success\"))\r\n\r\n      .catch(err => console.log(err));\r\n    }\r\n  },\r\n  mounted() {\r\n    if (localStorage.getItem(\"keranjangUser\")) {\r\n      try {\r\n        this.keranjangUser = JSON.parse(localStorage.getItem(\"keranjangUser\"));\r\n      } catch (e) {\r\n        localStorage.removeItem(\"keranjangUser\");\r\n      }\r\n    }\r\n  },\r\n    computed: {\r\n    totalHarga(){\r\n      return this.keranjangUser.reduce(function(items, data){\r\n        return items + data.price;\r\n      }, 0 );\r\n    },\r\n    tambahPajak(){\r\n        return (this.totalHarga*10)/100;\r\n    },\r\n    totalBiaya(){\r\n        return this.totalHarga + this.tambahPajak;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.img-cart{\r\n    width: 80px;\r\n    height: 65px;\r\n}\r\n</style>"]}]}